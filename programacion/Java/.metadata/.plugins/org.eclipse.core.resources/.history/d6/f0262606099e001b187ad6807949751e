package es.inmobiliaria;

public abstract class Inmueble {
	String direccion;
	float superficie;
	String propietario;
	
	
	
	public Inmueble(String direccion, float superficie, String propietario) {
		this.direccion = direccion;
		this.superficie = superficie;
		this.propietario = propietario;
	}
	
}
