package es.inmobiliaria;

public class Casa extends Habitable{
	private int plantas;

	public Casa(String direccion, float superficie, String propietario, int dormitorios, int baños, int plantas) {
		super(direccion, superficie, propietario, dormitorios, baños);
		this.plantas = plantas;
	}
	public int getPlantas() {
		return plantas;
	}

	public void setPlantas(int plantas) {
		this.plantas = plantas;
	}
	@Override
	public String toString() {
		return " y " + getPlantas() + "plantas, siendo esta una casa.";
	}

	
	
}
